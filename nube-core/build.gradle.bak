
description = ''
dependencies {
    compile group: 'org.springframework.data', name: 'spring-data-mongodb', version:'2.0.10.RELEASE'
    compile group: 'javax.validation', name: 'validation-api', version:'2.0.1.Final'
    compile(group: 'org.apache.httpcomponents', name: 'httpclient', version:'4.5.6') {
exclude(module: 'commons-logging')
    }
    compile group: 'org.springframework.cloud', name: 'spring-cloud-netflix-eureka-client', version:'2.1.0.M1'
    compile(group: 'com.netflix.eureka', name: 'eureka-client', version:'1.9.3') {
exclude(module: 'servlet-api')
exclude(module: 'commons-logging')
exclude(module: 'jsr305')
exclude(module: 'annotations')
    }
}
